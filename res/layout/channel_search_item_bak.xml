<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="@dimen/channel_search_item_height" >
    
    <TextView 
        android:id="@+id/channelSearchItemText"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:layout_alignParentLeft="true"
        android:layout_alignParentStart="true"
        android:layout_marginLeft="@dimen/channel_search_item_margin_left"
        android:layout_marginStart="@dimen/channel_search_item_margin_left"
        android:gravity="center_vertical"/>
    
    <ImageView 
        android:id="@+id/channelSearchItemNext"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:background="@drawable/common_next"
        android:paddingLeft="@dimen/channel_search_item_next_padding_left"
        android:paddingStart="@dimen/channel_search_item_next_padding_left"
        android:paddingTop="@dimen/channel_search_item_next_padding_top"
        android:paddingRight="@dimen/channel_search_item_next_padding_right"
        android:paddingEnd="@dimen/channel_search_item_next_padding_right"
        android:paddingBottom="@dimen/channel_search_item_next_padding_bottom"
        
        />

</RelativeLayout>
